{
  "name": "posigrade",
  "version": "0.0.6",
  "description": "A library to simplify pg query handling - not an ORM",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "./node_modules/.bin/rimraf dist && tsc",
    "ci:test": "npm run typecheck && npm run test -- --ci && npm run lint",
    "clean": "rm -rf dist .cache node_modules coverage",
    "lint": "eslint './src/**/*.ts'",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "postgres",
    "pg"
  ],
  "author": "Benoit Lemoine",
  "license": "MIT",
  "devDependencies": {
    "@types/dockerode": "2.5.24",
    "@types/jest": "25.1.3",
    "@types/node": "13.7.7",
    "@types/pg": "7.14.1",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "prettier": "1.19.1",
    "rimraf": "^3.0.2",
    "testcontainers": "2.6.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "pg": "7.18.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blemoine/posigrade.git"
  }
}
